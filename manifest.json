{
  "name": "TweetPuller by Jane Ullah",
  "version": "0.1",
  "manifest_version": 2,
  "description": "Pulls last 10 tweets from the username provided to the program.",
  "browser_action": {
      "default_icon": "notepad.png",
      "default_popup": "tweets.html"
  },
  "omnibox": { "keyword" : "tweetpuller" },
  "icons": {
        "16": "notepad.png"
    },
  "options_page":"options.html",
  "permissions": [
  "http://*.twitter.com/",
  "https://*.twitter.com/",
  "http://*.google.com/",
  "https://*.google.com/",
  "http://*.googleapis.com/",
  "https://*.googleapis.com/",
  "unlimitedStorage"
],
   "content_security_policy": "script-src 'self' https://ssl.google-analytics.com https://ajax.googleapis.com; object-src 'self'"
}
