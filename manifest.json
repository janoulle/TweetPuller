{
  "name": "DawgTransit",
  "version": "0.1",
  "manifest_version": 2,
  "description": "Pulls tweets from @dawgtransit",
  "browser_action": {
      "default_icon": "icon.png",
      "default_popup": "popup.html"
  },
  "permissions": [
  "https://*.twitter.com/",
  "http://*.google.com/",
  "https://*.google.com/",
  "unlimitedStorage"
],
   "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'"
}
